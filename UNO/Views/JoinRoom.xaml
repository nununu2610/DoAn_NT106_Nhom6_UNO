<Window x:Class="UNO.Views.JoinRoom"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UNO.Views"
        mc:Ignorable="d"
        Title="JoinRoom" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Images/Join (3).png"/>
        </Grid.Background>
        <TextBox x:Name="txtNameJoin" HorizontalAlignment="Left" Height="40" Margin="390,134,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="340"  FontSize="24"/>
        <TextBox x:Name="txtIP" HorizontalAlignment="Left" Height="40" Margin="390,244,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="340" FontSize="24"/>
        <Button x:Name="btnBack" Content="Button" HorizontalAlignment="Left" Height="65" Margin="60,334,0,0" VerticalAlignment="Top" Width="130" Background="Transparent" BorderBrush="Transparent" Foreground="Transparent" Opacity="0" Click="btnBack_Click"/>

    </Grid>
</Window>
