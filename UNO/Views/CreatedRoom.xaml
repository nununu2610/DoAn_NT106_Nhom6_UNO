        <Button x:Name="btnPlay" Background="Transparent" BorderBrush="Transparent" Foreground="Transparent"  Opacity="0"  Content="Button" HorizontalAlignment="Left" Height="65" Margin="595,334,0,0" VerticalAlignment="Top" Width="125"/>

